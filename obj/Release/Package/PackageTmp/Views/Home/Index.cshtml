						<button onclick="myJsFunction()">Get FizzBuzz</button>
						<div id=out_put></div>
					</div>
					<br/>
					<pre class="brush: js;">
				function myJsFunction() {
				var div = document.getElementById('out_put');
				inptnum = document.getElementById('input1').value;
				inptnum2 = document.getElementById('input2').value;
				j = inptnum;
				k = inptnum2;
				for (var i = 1; i <= 100; i++) {
				if (i % j == 0 && i % k == 0) {
					div.innerHTML += "FizzBuzz";
				} else if (i % j == 0) {
					div.innerHTML += "Fizz";
				} else if (i % k == 0) {
					div.innerHTML += "Buzz";
				} else {
					div.innerHTML += i;
				}
				div.innerHTML += '<br>';
				}
				}
					</pre>
				</div>
